  <section>
<% projects.forEach(project => { %>
    <article>
      <h2><%= project.node.name %></h2>
      <p><%= project.node.description %></p>
      <% if (project.node.homepageUrl !== null) { %>
      <a class="live" href="<%= project.node.homepageUrl %>" alt="Watch <%= project.node.name %> in action"><span>Watch in action</span></a>
      <% } %>
      <a class="github" href="<%= project.node.url %>" alt="Watch <%= project.node.name %> on GitHub"><span>Browse on GitHub</span></a>
    </article>

<% }) %>
  </section>

